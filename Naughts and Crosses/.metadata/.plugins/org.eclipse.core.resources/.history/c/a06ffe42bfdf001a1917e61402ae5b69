package player.bot;

public class HalfNode extends Node {

	public HalfNode(int size) {
		super(size);
	}
	
	public HalfNode(double[] weights) {
		super(weights);
	}
	
	public HalfNode(String weights) {
		super(weights);
	}

	@Override
	public double function(double in) {
		System.out.println("here");
		return in>0.5? 1: 0;
	}
	
}
