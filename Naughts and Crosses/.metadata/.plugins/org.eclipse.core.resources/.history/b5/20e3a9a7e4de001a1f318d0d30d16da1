package game;

import player.Player;

public class Game {

	static Board board;
	Player player1;
	Player player2;
	
	public Result play(Player player1, Player player2) {
		while (true) {
			player1.takeTurn(board);
			player2.takeTurn(board);
		}
		
		return null;
		
	}
	
	public static void main(String[] args) {

	}
	
}
