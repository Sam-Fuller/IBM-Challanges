{
    "name": "sal-language-support",
    "displayName": "Sal Language Support",
    "description": "Sal language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sal",
            "aliases": ["Sal", "sal"],
            "extensions": ["sal"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sal",
            "scopeName": "source.sal",
            "path": "./syntaxes/sal.tmLanguage.json"
        }]
    }
}